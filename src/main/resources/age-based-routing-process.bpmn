<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" 
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" 
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" 
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI" 
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn" 
                  id="Definitions_age_routing_simplified" 
                  targetNamespace="http://bpmn.io/schema/bpmn" 
                  exporter="Camunda Modeler" 
                  exporterVersion="5.0.0">
  
  <bpmn:process id="age-based-routing-process" name="Simplified Age-Based Routing Process" isExecutable="true" camunda:historyTimeToLive="P30D">
    
    <!-- Start Event -->
    <bpmn:startEvent id="start_event" name="Start Process">
      <bpmn:outgoing>flow_start_to_task</bpmn:outgoing>
    </bpmn:startEvent>
    
    <!-- Single Routing Task (Simplified) -->
    <bpmn:serviceTask id="routing_task" name="Process Person with Age-Based Routing" camunda:delegateExpression="${ageBasedRoutingDelegate}">
      <bpmn:incoming>flow_start_to_task</bpmn:incoming>
      <bpmn:outgoing>flow_task_to_end</bpmn:outgoing>
    </bpmn:serviceTask>
    
    <!-- End Event -->
    <bpmn:endEvent id="end_event" name="Process Complete">
      <bpmn:incoming>flow_task_to_end</bpmn:incoming>
    </bpmn:endEvent>
    
    <!-- Sequence Flows (Simplified) -->
    <bpmn:sequenceFlow id="flow_start_to_task" sourceRef="start_event" targetRef="routing_task" />
    <bpmn:sequenceFlow id="flow_task_to_end" sourceRef="routing_task" targetRef="end_event" />
    
  </bpmn:process>
  
  <!-- Simplified Diagram -->
  <bpmndi:BPMNDiagram id="BPMNDiagram_simplified">
    <bpmndi:BPMNPlane id="BPMNPlane_simplified" bpmnElement="age-based-routing-process">
      
      <!-- Start Event -->
      <bpmndi:BPMNShape id="start_shape" bpmnElement="start_event">
        <dc:Bounds x="180" y="120" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="168" y="163" width="66" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Routing Task -->
      <bpmndi:BPMNShape id="routing_shape" bpmnElement="routing_task">
        <dc:Bounds x="300" y="98" width="100" height="80" />
        <bpmndi:BPMNLabel />
      </bpmndi:BPMNShape>
      
      <!-- End Event -->
      <bpmndi:BPMNShape id="end_shape" bpmnElement="end_event">
        <dc:Bounds x="480" y="120" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="455" y="163" width="86" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      
      <!-- Sequence Flows -->
      <bpmndi:BPMNEdge id="start_edge" bpmnElement="flow_start_to_task">
        <di:waypoint x="216" y="138" />
        <di:waypoint x="300" y="138" />
      </bpmndi:BPMNEdge>
      
      <bpmndi:BPMNEdge id="end_edge" bpmnElement="flow_task_to_end">
        <di:waypoint x="400" y="138" />
        <di:waypoint x="480" y="138" />
      </bpmndi:BPMNEdge>
      
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
  
</bpmn:definitions>